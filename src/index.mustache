<!DOCTYPE html>
<html lang="en">
  <head>
    <title>syle-resume</title>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta name="format-detection" content="telephone=no" />
    <meta name="keywords"   content="resume, synle, sy le, front end engineer, full stack engineer.">
    <meta name="Description" content="{{Summary}}" />
    <link
      rel="icon"
      href="data:image/svg+xml,%3Csvg%20xmlns%3D'http%3A%2F%2Fwww.w3.org%2F2000%2Fsvg'%20viewBox%3D'0%200%2016%2016'%3E%3Ctext%20x%3D'0'%20y%3D'16'%3EðŸ§‘%3C%2Ftext%3E%3C%2Fsvg%3E" />
    <link rel="stylesheet" href="/index.css" />
  </head>
  <body>
    <!-- Contact Info -->
    <!--   Printer only   -->
    <header class="visible-print">
      <h1 id="full_name">{{FullName}}</h1>
      <div class="page-header visible-print-flex">
        <span id="location">{{Location}}</span>
        <a id="contact_phone_number" href="tel:1-{{PhoneNumber}}" class="info-phone cap">{{PhoneNumber}}</a>
        <a id="contact_email" href="mailto:{{Email}}" class="info-email cap">{{Email}}</a>
        <a href="https://{{HomePageURL}}" class="info-github visible-print cap">{{HomePageURL}}</a>
        <a href="https://{{LinkedinURL}}" class="info-linkedin visible-print cap">{{LinkedinURL}}</a>
      </div>
    </header>

    <header class="page-header visible-desktop visible-mobile mb4">
      <h1 id="full_name" class="bold1">{{FullName}}</h1>
      <div id="location">{{Location}}</div>
      <div><a id="contact_phone_number" href="tel:1-{{PhoneNumber}}" class="info-phone cap">{{PhoneNumber}}</a></div>
      <div><a id="contact_email" href="mailto:{{Email}}" class="info-email cap">{{Email}}</a></div>
    </header>

    <!--   Screen only - social links   -->
    <section class="social-links visible-screen-flex">
      <a href="//{{LinkedinURL}}" class="info-linkedin cap" title="Linkedin Profile"> Linkedin </a>
      <a href="//{{GithubURL}}" class="info-github cap ms4" title="Github Profile"> Github </a>
      <a href="{{ResumeURL}}" download="" class="cap" title="Download Resume"> Resume </a>
    </section>

    <!-- summary  -->
    <div class="section-header z3">
      Summary:
    </div>
    <div class="section-content">
      <div class="row c1">{{Summary}}</div>
    </div>


    <!-- Educations -->
    <div class="section-header z3">
      Education:
    </div>
    {{#Education}}
      <article>
        {{#rows}}
          <header class="section-row-title">
            <div>{{degree}}, {{program}}</div>
            <div>{{school}}</div>
            <div>{{graduated}}</div>
          </header>
          <ul class="section-row-content">
            {{#items}}
              <li class="row"><span>&#183;</span>{{{.}}}</li>
            {{/items}}
          </ul>
        {{/rows}}
      </article>
    {{/Education}}

    <!-- Experience -->
    <div class="section-header">
      Work Experience:
    </div>
    {{#Experience}}
      <article>
        {{#rows}}
          <header class="section-row-title">
            <div>{{title}} at {{company}}</div>
            <div>{{location}}</div>
            <div>
              <span class="visible-screen-inline-block"><span aria-label="from">From</span> {{from}} <span aria-label="to">To</span> {{to}}</span>
              <span class="visible-print-inline-block">{{from}} <span aria-label="to">-</span> {{to}}</span>
            </div>
          </header>
          <ul class="section-row-content">
            {{#items}}
              <li class="row"><span>&#183;</span>{{{.}}}</li>
            {{/items}}
          </ul>
        {{/rows}}
      </article>
    {{/Experience}}

    <!-- Side Project -->
    <div class="section-header z4">
      Side Projects:
    </div>
    <article>
      <ul class="section-row-content">
        {{#SideProjects}}
          {{#rows}}
              <li class="row"><span>&#183;</span><a href="{{link}}">{{label}}:</a> {{{description}}}</li>
          {{/rows}}
        {{/SideProjects}}
      </ul>
    </article>

    <!-- Skills -->
    <div class="section-header z5">
      Skills:
    </div>
    <article>
      <ul class="section-row-content">
        {{#Skills}}
          {{#rows}}
            <li class="row"><span>&#183;</span><strong class='c0'>{{label}}:</strong> {{{description}}}</li>
          {{/rows}}
        {{/Skills}}
      </ul>
    </article>
  </body>

  <!-- minor scripts -->
  <script>
    (function () {
      // set up anchor rel attr
      try {
        for(const anchorTag of document.querySelectorAll('a')){
          anchorTag.setAttribute('target', '_blank');
          anchorTag.setAttribute('rel', 'noopener noreferrer');
        }
      } catch (e) {}

      // dark mode toggle
      let isDarkMode = false;
      try {
        isDarkMode = window.matchMedia('(prefers-color-scheme: dark)').matches;
      } catch (e) {}
      document.querySelector('html').classList.toggle('light-theme', !isDarkMode);
    })();
  </script>
</html>
